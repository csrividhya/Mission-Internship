import java.io.*;
import java.util.*;
public class Main
{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        int a, b;
        System.out.println("enter a");
        a=s.nextInt();
        int c =0;
        while(a>0)
        {
            if((a&1)==1)
                c++;
            a=a>>1;
        }
       System.out.println("number of set bits= "+c);
    }
    
}
